<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kinkong">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@drawable/app_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />

            </intent-filter>
        </activity>

        <activity android:name=".CountDownActivity" android:screenOrientation="portrait"/>
        <activity android:name=".QuestionActivity" android:screenOrientation="portrait"/>
        <activity android:name=".AccountInfoActivity" android:screenOrientation="portrait"/>

        <activity android:name=".KinTutorial" android:screenOrientation="portrait" android:theme="@style/TranslucentDecor"/>
        <activity android:name=".QuestionVideoActivity" android:screenOrientation="portrait" android:theme="@style/TranslucentDecor"/>

    </application>

</manifest>